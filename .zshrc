[ -f $HOME/.config/zsh/zshrc ] && source "$HOME/.config/zsh/zshrc"
[ -f $HOME/.local/zshrc      ] && source "$HOME/.local/zshrc"

[ -f $HOME/.config/sourcefile ] && source "$HOME/.config/sourcefile"
[ -f $HOME/.local/sourcefile  ] && source "$HOME/.local/sourcefile"

[ -f $HOME/.config/alias ] && source "$HOME/.config/alias"
[ -f $HOME/.local/alias  ] && source "$HOME/.local/alias"

